<ul>
	<% for (let index = 0; index < navBar.items.length; index++) { %>
		<% const item = navBar.items[index] %>
		<% if (item.type == "page") { %>
			<% if (index == navBar.active) { %>
				<li><a id="active" href=<%= item.URL %> > <%= item.name %></a></li>
			<% } else { %>
				<% if (item.float == "right") { %>
					<li style="float:right"><a href=<%= item.URL %> > <%= item.name %></a></li>
				<% } else { %>
					<li><a href=<%= item.URL %> > <%= item.name %></a></li>
				<% } %>
			<% }Â %>
		<% } %>
	<% } %>
</ul>